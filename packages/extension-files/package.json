{
  "name": "@positronic/extension-files",
  "version": "0.0.1",
  "description": "A file extension for Positronic",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "tsc": "tsc --project tsconfig.json",
    "swc": "swc . -d dist",
    "build": "npm run tsc && npm run swc",
    "clean": "rm -rf tsconfig.tsbuildinfo dist"
  },
  "dependencies": {
    "@positronic/core": "^0.0.1"
  }
}
