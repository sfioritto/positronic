{
  "name": "@positronic/backend",
  "version": "0.0.1",
  "description": "Backend interface and test suite for Positronic framework",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./test-suite": {
      "types": "./dist/test-suite/index.d.ts",
      "import": "./dist/test-suite/index.js"
    }
  },
  "scripts": {
    "tsc": "tsc --project tsconfig.json",
    "swc": "swc src -d dist",
    "build": "npm run tsc && npm run swc",
    "clean": "rm -rf tsconfig.tsbuildinfo dist node_modules"
  },
  "peerDependencies": {
    "@positronic/core": "^0.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
