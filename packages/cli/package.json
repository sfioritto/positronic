{
  "name": "@positronic/cli",
  "version": "0.0.22",
  "publishConfig": {
    "access": "public"
  },
  "description": "A CLI for AI Brains",
  "type": "module",
  "license": "MIT",
  "bin": {
    "positronic": "./dist/src/positronic.js",
    "px": "./dist/src/positronic.js"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "tsc": "tsc --project tsconfig.json",
    "swc": "swc src -d dist",
    "build": "npm run tsc && npm run swc",
    "postbuild": "chmod +x ./dist/src/positronic.js",
    "clean": "rm -rf tsconfig.tsbuildinfo dist node_modules"
  },
  "dependencies": {
    "@positronic/core": "^0.0.22",
    "@positronic/spec": "^0.0.22",
    "@positronic/template-new-project": "^0.0.22",
    "caz": "^2.0.0",
    "chokidar": "^3.6.0",
    "eventsource": "^3.0.6",
    "ink": "^5.2.1",
    "istextorbinary": "^9.5.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/react": "^19.1.3",
    "@types/yargs": "^17.0.33",
    "ink-testing-library": "^4.0.0",
    "nock": "^14.0.5"
  }
}
