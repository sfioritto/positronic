{
  "name": "@positronic/cli",
  "version": "0.0.1",
  "description": "A CLI for AI Workflows",
  "type": "module",
  "license": "MIT",
  "bin": {
    "positronic": "./dist/src/positronic.js",
    "px": "./dist/src/positronic.js"
  },
  "scripts": {
    "tsc": "tsc --project tsconfig.json",
    "swc": "swc src -d dist",
    "build": "npm run tsc && npm run swc && mkdir -p dist/templates && cp -R templates/. dist/templates",
    "postbuild": "chmod +x ./dist/src/positronic.js",
    "clean": "rm -rf tsconfig.tsbuildinfo dist node_modules"
  },
  "dependencies": {
    "@positronic/client-anthropic": "^0.0.1",
    "@positronic/core": "^0.0.1",
    "@positronic/resources": "^0.0.1",
    "@positronic/shell": "^0.0.1",
    "@positronic/sqlite": "^0.0.1",
    "better-sqlite3": "^11.7.2",
    "ts-node": "^10.9.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/yargs": "^17.0.32",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.101",
    "typescript": "^5.3.3",
    "chokidar": "^3.6.0"
  }
}
