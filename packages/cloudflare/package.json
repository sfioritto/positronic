{
  "name": "@positronic/cloudflare",
  "version": "0.0.12",
  "publishConfig": {
    "access": "public"
  },
  "description": "Cloudflare bindings for Positronic brains",
  "type": "module",
  "exports": {
    ".": {
      "node": {
        "types": "./dist/types/node-index.d.ts",
        "import": "./dist/src/node-index.js"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/src/index.js"
      }
    }
  },
  "main": "./dist/src/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "tsc": "tsc --project tsconfig.json",
    "swc": "swc src -d dist",
    "build": "npm run tsc && npm run swc",
    "clean": "rm -rf tsconfig.tsbuildinfo dist"
  },
  "dependencies": {
    "@positronic/core": "^0.0.12",
    "@positronic/spec": "^0.0.12",
    "@positronic/template-new-project": "^0.0.12",
    "aws4fetch": "^1.0.18",
    "caz": "^2.0.0",
    "cron-schedule": "^5.0.4",
    "dotenv": "^16.0.3",
    "hono": "^4.2.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250415.0",
    "@types/node": "^22.14.1",
    "@types/uuid": "^9.0.8"
  }
}
