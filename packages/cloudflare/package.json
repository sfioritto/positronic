{
  "name": "@positronic/cloudflare",
  "version": "0.0.1",
  "description": "Cloudflare bindings for Positronic workflows",
  "type": "module",
  "exports": {
    ".": {
      "node": {
        "types": "./dist/types/node-index.d.ts",
        "import": "./dist/src/node-index.js"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/src/index.js"
      }
    }
  },
  "main": "./dist/src/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "tsc": "tsc --project tsconfig.json",
    "swc": "swc src -d dist",
    "build": "npm run tsc && npm run swc",
    "clean": "rm -rf tsconfig.tsbuildinfo dist"
  },
  "dependencies": {
    "@positronic/core": "^0.0.1",
    "@positronic/spec": "^0.0.1",
    "@positronic/template-new-project": "^0.0.1",
    "caz": "^2.0.0",
    "dotenv": "^16.0.3",
    "hono": "^4.2.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250415.0",
    "@types/node": "^22.14.1",
    "@types/uuid": "^9.0.8"
  }
}
