{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ESNext"],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["@cloudflare/workers-types"],
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "@positronic/cloudflare/*": ["../src/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}