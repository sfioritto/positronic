/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "test-project",
  "main": "src/index.ts",
  "compatibility_date": "2025-04-04",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "preview_urls": true,
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["BrainRunnerDO", "MonitorDO", "ScheduleDO"]
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "BrainRunnerDO",
        "name": "BRAIN_RUNNER_DO"
      },
      {
        "class_name": "MonitorDO",
        "name": "MONITOR_DO"
      },
      {
        "class_name": "ScheduleDO",
        "name": "SCHEDULE_DO"
      }
    ]
  },
  "vars": {
    "IS_TEST": "true",
    "NODE_ENV": "test",
    "CLOUDFLARE_API_TOKEN": "test-api-token",
    "CLOUDFLARE_ACCOUNT_ID": "test-account-id",
    "CF_SCRIPT_NAME": "test-project",
    "ROOT_PUBLIC_KEY": "{\"kty\":\"OKP\",\"crv\":\"Ed25519\",\"x\":\"fYW1WaT583-Y_WWP7_lEmKa132Ue_RoEPcSoai-3kzk\"}"
  },
  "r2_buckets": [
    {
      "binding": "TEST_RESOURCES_BUCKET",
      "bucket_name": "positronic-test-resources"
    },
    {
      "binding": "RESOURCES_BUCKET",
      "bucket_name": "positronic-test-resources"
    }
  ],
  "observability": {
    "enabled": true
  }
}
