{
  "name": "@positronic/gen-ui-components",
  "version": "0.0.52",
  "publishConfig": {
    "access": "public"
  },
  "description": "Default UI components for Positronic generative UI",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "tsc": "tsc --project tsconfig.json",
    "swc": "swc src -d dist/src --strip-leading-paths",
    "bundle": "esbuild src/bundle.ts --bundle --external:react --external:react-dom --format=iife --global-name=PositronicComponents --outfile=dist/components.js",
    "generate-bundle-export": "node scripts/generate-bundle-export.js",
    "build": "npm run bundle && npm run generate-bundle-export && npm run tsc && npm run swc",
    "clean": "rm -rf tsconfig.tsbuildinfo dist node_modules src/generated-bundle.ts"
  },
  "dependencies": {
    "@positronic/core": "^0.0.52",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.0",
    "esbuild": "^0.24.0"
  }
}
